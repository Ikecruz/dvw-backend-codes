{
    "compilerOptions": {
      "experimentalDecorators": true,
      "target": "es2016",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strictPropertyInitialization": false,
      "strict": true, 
      "outDir": "./dist",
      "rootDir": "src",
      "noImplicitAny": false,
    },
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "node_modules"
    ]
}